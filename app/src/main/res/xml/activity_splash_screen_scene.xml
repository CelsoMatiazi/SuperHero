<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:autoTransition="animateToEnd"
        motion:duration="1500">
        <KeyFrameSet>
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/img_1"
            motion:layout_constraintEnd_toStartOf="@id/img_2"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="0dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/img_2"
            motion:layout_constraintEnd_toStartOf="@id/img_3"
            motion:layout_constraintStart_toEndOf="@id/img_1"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp" />
        <Constraint
            android:id="@+id/img_3"
            motion:layout_constraintEnd_toStartOf="@id/img_4"
            motion:layout_constraintStart_toEndOf="@id/img_2"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="0dp"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/img_4"
            motion:layout_constraintEnd_toStartOf="@id/img_5"
            motion:layout_constraintStart_toEndOf="@id/img_3"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp" />
        <Constraint
            android:id="@+id/img_5"
            motion:layout_constraintEnd_toStartOf="@id/img_6"
            motion:layout_constraintStart_toEndOf="@id/img_4"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="0dp"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/img_6"
            motion:layout_constraintEnd_toStartOf="@id/img_7"
            motion:layout_constraintStart_toEndOf="@id/img_5"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp" />
        <Constraint
            android:id="@+id/img_7"
            motion:layout_constraintEnd_toStartOf="@id/img_8"
            motion:layout_constraintStart_toEndOf="@id/img_6"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="0dp"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/img_8"
            motion:layout_constraintEnd_toStartOf="@id/img_9"
            motion:layout_constraintStart_toEndOf="@id/img_7"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp" />
        <Constraint
            android:id="@+id/img_9"
            motion:layout_constraintEnd_toStartOf="@id/img_10"
            motion:layout_constraintStart_toEndOf="@id/img_8"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="0dp"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/img_10"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/img_9"
            android:layout_width="40dp"
            android:layout_height="240dp"
            android:alpha="0.9"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_chainStyle="spread_inside"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp" />
    </ConstraintSet>
</MotionScene>